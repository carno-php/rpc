{
  "name": "carno-php/rpc",
  "license": "MIT",
  "require": {
    "moolex/protobuf-php": "^3.5",
    "carno-php/container": "^1.0",
    "carno-php/coroutine": "^1.0",
    "carno-php/promise": "^1.0",
    "carno-php/chain": "^1.0"
  },
  "suggest": {
    "ext-ast": "Service detecting via AST"
  },
  "autoload": {
    "psr-4": {
      "Carno\\RPC\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Carno\\RPC\\Tests\\": "tests",
      "Carno\\RPC\\Tests\\Mocked\\": "tests/mocked-app/src",
      "Mocked\\App\\Tests\\Contracts\\": "tests/mocked-app/api"
    },
    "files": ["tests/mocked-app/bootstrap.php"]
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
